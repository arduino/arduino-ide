    "settings.headerForeground": "#00ffff",
    "tree.indentGuidesStroke": "#1a1a2e",
    "tab.unfocusedActiveForeground": "#666666",
    "tab.inactiveBackground": "#0f1419",
    "tab.activeBackground": "#0a0e14",
    "tab.activeForeground": "#00ffff",
    "textLink.foreground": "#00ffff",
    "errorForeground": "#ff0066"
  },
  "tokenColors": [
    {
      "name": "Default text",
      "settings": { "foreground": "#d4d4d4" }
    },
    {
      "name": "Comments",
      "scope": "comment",
      "settings": { "foreground": "#6A9955", "fontStyle": "italic" }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": { "foreground": "#569CD6", "fontStyle": "bold" }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "meta.function-call.c", "meta.function.c"],
      "settings": { "foreground": "#DCDCAA" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.name", "variable.other"],
      "settings": { "foreground": "#9CDCFE" }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted.double", "string.quoted.single"],
      "settings": { "foreground": "#CE9178" }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#B5CEA8" }
    },
    {
      "name": "Types",
      "scope": ["entity.name.type", "support.type"],
      "settings": { "foreground": "#4EC9B0", "fontStyle": "italic" }
    },
    {
      "name": "Punctuation/Operators",
      "scope": ["punctuation", "keyword.operator"],
      "settings": { "foreground": "#d4d4d4" }
    }
  ]
}
